{
  "name": "ng-webpack-skel",
  "version": "0.0.1",
  "description": "simple template for SPA projects",
  "scripts": {
    "setup": "rimraf dist && mkdirp dist",
    "test": "karma start karma.config.js",
    "test:ci": "karma start karma.config.ci.js",
    "watch:dev": "WEBPACK_CONTEXT=dev webpack --watch --display-error-details --progress --colors",
    "watch:dev:bs": "parallelshell \"npm run watch:dev -s\" \"npm run bs -s\"",
    "watch:test:dev": "parallelshell \"npm test -s\" \"npm run watch:dev -s\"",
    "prebuild:prod": "npm run setup -s",
    "build:prod": "WEBPACK_CONTEXT=prod webpack --display-error-details --progress --colors",
    "prebuild:dev": "npm run setup -s",
    "build:dev": "WEBPACK_CONTEXT=dev webpack --display-error-details --progress --colors",
    "bs": "browser-sync start --config bs.config.js",
    "flush": "rimraf node_modules/* && npm cache clean && npm i"
  },
  "dependencies": {
    "angular": "1.4.0",
    "angular-resource": "1.4.0",
    "angular-route": "1.4.0"
  },
  "devDependencies": {
    "angular-mocks": "1.4.0",
    "autoprefixer-core": "~5.1.11",
    "browser-sync": "~2.6.4",
    "browser-sync-spa": "~1.0.2",
    "css-loader": "~0.10.1",
    "extract-text-webpack-plugin": "~0.5.0",
    "html-webpack-plugin": "~1.1.0",
    "jshint": "~2.7.0",
    "jshint-loader": "~0.8.3",
    "json-loader": "~0.5.1",
    "karma": "~0.12.31",
    "karma-chrome-launcher": "~0.1.8",
    "karma-cli": "0.0.4",
    "karma-jasmine": "~0.3.5",
    "karma-jasmine-html-reporter": "~0.1.8",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-sourcemap-loader": "~0.3.4",
    "karma-webpack": "~1.3.1",
    "mkdirp": "~0.5.1",
    "parallelshell": "~1.1.1",
    "postcss": "~4.1.11",
    "postcss-bem-linter": "~0.3.0",
    "postcss-import": "~5.2.2",
    "postcss-loader": "~0.4.3",
    "pretty-hrtime": "~0.2.1",
    "raw-loader": "~0.5.1",
    "require-dir": "~0.1.0",
    "rimraf": "~2.3.3",
    "style-loader": "~0.8.1",
    "url-loader": "~0.5.5",
    "webpack": "~1.8.5",
    "webpack-dev-server": "~1.8.0"
  }
}
